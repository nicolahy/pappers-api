{
  "name": "Kepler <--> Pappers Extension",
  "description": "This extension allows you to fill Kepler data with Pappers API data",
  "version": "2.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/pappers_logo_x16.png",
    "32": "images/pappers_logo_x32.png",
    "64": "images/pappers_logo_x64.png",
    "128": "images/pappers_logo_x128.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "images/pappers_logo.png"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": false
  },
  "web_accessible_resources": [{
    "resources": ["images/placeholder_1.png", "images/placeholder_2.png"],
    "matches": ["<all_urls>"]
  }]
}