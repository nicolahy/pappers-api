{
  "name": "Pappers API Extension",
  "description": "This extension allows you to fill Kepler forms with Pappers API 2.11.0 data.",
  "version": "3.3",
  "manifest_version": 3,
  "icons": {
    "16": "images/pappers_logo.png",
    "32": "images/pappers_logo.png",
    "64": "images/pappers_logo.png",
    "128": "images/pappers_logo.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "images/pappers_logo.png"
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": false
  },
  "web_accessible_resources": [{
    "resources": ["images/placeholder_1.png", "images/placeholder_2.png"],
    "matches": ["<all_urls>"]
  }]
}